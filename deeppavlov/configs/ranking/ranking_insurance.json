{
  "dataset_reader": {
    "name": "insurance_reader",
    "data_path": "./insurance_data"
  },
  "dataset_iterator": {
    "name": "ranking_iterator",
    "seed": 243
  },
  "chainer": {
    "in": ["x"],
    "in_y": ["y"],
    "pipe": [
      {
        "id": "insurance_vocab",
        "name": "ranking_vocab",
        "save_path": "vocabs/",
        "load_path": "vocabs/",
        "use_matrix": false,
        "num_ranking_samples": 10,
        "max_sequence_length": 200,
        "tokenizer": {"name": "split_tokenizer"},
        "embedder": "random",
        "embedding_dim": 300,
        "fit_on": ["x"],
        "in": ["x"],
        "out": ["x_proc"]
      },
      {
        "in": ["x_proc"],
        "in_y": ["y"],
        "out": ["y_predicted"],
        "name": "ranking_model",
        "device_num": 0,
        "train_now": true,
        "len_vocab": "#insurance_vocab.len_vocab",
        "len_char_vocab": "#insurance_vocab.len_char_vocab",
        "embeddings_path": "insurance_embeddings/wiki.en.bin",
        "use_matrix": "#insurance_vocab.use_matrix",
        "emb_matrix": "#insurance_vocab.emb_matrix",
        "max_sequence_length": "#insurance_vocab.max_sequence_length",
        "embedding_dim": "#insurance_vocab.embedding_dim",
        "seed": 243,
        "reccurent": "bilstm",
        "max_pooling": true,
        "shared_weights": true,
        "hidden_dim": 300,
        "learning_rate": 1e-3,
        "triplet_mode": true,
        "margin": 0.1,
        "save_path": "insurance_ranking/model_weights.h5",
        "load_path": "insurance_ranking/model_weights.h5",
        "update_embeddings": false,
        "interact_pred_num": 3
      }
    ],
    "out": ["y_predicted"]
  },
  "train": {
    "epochs": 200,
    "batch_size": 64,
    "pytest_max_batches": 2,
    "metrics": ["r@1", "rank_response", "loss"],
    "validation_patience": 5,
    "val_every_n_epochs": 1
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf.txt",
      "../dp_requirements/gensim.txt"
    ],
    "labels": {
      "telegram_utils": "RankingModel",
      "server_utils": "Ranker"
    },
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/insurance_ranking.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/datasets/insuranceQA-master.zip",
        "subdir": "insurance_data"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki.en.bin",
        "subdir": "insurance_embeddings"
      }
    ]
  }
}